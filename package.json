{
  "private": true,
  "name": "orion",
  "version": "1.0.0",
  "description": "the autodesk ui library",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "NODE_ENV=production lerna run build && npm run copy-package-builds",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "copy-package-builds": "node ./scripts/copy-package-builds.js",
    "deploy-master": "npm run deploy -- --build-id=master",
    "deploy-snapshot": "npm run deploy -- --build-id $CIRCLE_BUILD_NUM",
    "deploy": "node ./scripts/deploy.js --gh-token $GITHUB_TOKEN --gh-sha $CIRCLE_SHA1",
    "ensure-copyright": "node ./scripts/ensure-copyright.js --dir ./",
    "set-marketing-to-root": "node ./scripts/set-package-to-root.js -p marketing-playground",
    "build-lib": "NODE_ENV=development lerna run lib --stream",
    "test-ci": "npm run ensure-copyright && npm run lint && lerna run test-ci --ignore hig.web --stream",
    "test": "npm run ensure-copyright && npm run lint && lerna run test --stream",
    "start": "lerna run start --no-sort --stream --concurrency 50",
    "start-playground": "node scripts/start-playground.js",
    "publish-canary": "lerna publish --canary --yes --exact",
    "postinstall": "git submodule sync && git submodule update --init"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orion-ui/orion.git"
  },
  "author": "Autodesk Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/orion-ui/orion/issues"
  },
  "homepage": "https://github.com/orion-ui/orion#readme",
  "dependencies": {},
  "devDependencies": {
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addons": "^1.6.1",
    "aws-sdk": "^2.7.11",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-preset-react-app": "^2.0.1",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "codacy-coverage": "^2.0.0",
    "commander": "^2.9.0",
    "enzyme": "^2.6.0",
    "es6-promise": "^4.0.5",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-prettier": "^1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.8.0",
    "github": "^9.2.0",
    "glob": "^7.1.1",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "lerna": "2.0.0-rc.4",
    "nock": "^9.0.2",
    "nyc": "^10.1.2",
    "prettier": "^0.22.0",
    "proxyquire": "^1.7.11",
    "raf-stub": "^2.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "s3": "^4.4.0",
    "shelljs": "^0.7.5",
    "sinon-chai": "^2.8.0",
    "web-component-tester-istanbul": "^0.10.0"
  }
}