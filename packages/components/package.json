{
  "name": "@orion-ui/components",
  "version": "1.0.3",
  "description": "orion ui components",
  "main": "lib/index.js",
  "author": "Autodesk Inc.",
  "license": "Apache-2.0",
  "jest": {
    "coverageDirectory": "coverage/jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  },
  "scripts": {
    "prepublish": "bower install",
    "lib": "babel src --out-dir lib --copy-file",
    "build": "NODE_ENV=production npm run lib",
    "start": "NODE_ENV=development npm run lib -- --watch",
    "jest-coverage": "jest --coverage",
    "jest": "jest --watch",
    "browserify": "node scripts/browserify.js",
    "wct": "wct",
    "wct-sauce": "wct --plugin sauce --skip-plugin local && node script/recmap-wct-coverage.js",
    "lint": "eslint .",
    "test-ci": "node scripts/test-ci.js"
  },
  "dependencies": {
    "@orion-ui/style": "^1.0.3",
    "object.entries": "^1.0.4"
  },
  "peerDependencies": {
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^19.0.0",
    "babelify": "^7.3.0",
    "bower": "^1.8.0",
    "browserify": "^13.3.0",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "gemini": "^4.14.3",
    "gemini-sauce": "^1.0.0",
    "istanbul-combine": "^0.3.0",
    "istanbul-merge": "^1.1.1",
    "jest": "^19.0.2",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "nyc": "^10.1.2",
    "raf-stub": "^2.0.0",
    "remap-istanbul": "^0.9.5",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.9.0",
    "web-component-tester": "4.2.2",
    "web-component-tester-istanbul": "^0.10.0"
  }
}
